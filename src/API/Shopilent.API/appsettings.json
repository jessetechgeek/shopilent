{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "AutoMigrate": false
  },
  "Seeding": {
    "AutoSeed": false,
    "CleanBeforeSeed": true,
    "Counts": {
      "Admins": 2,
      "Managers": 5,
      "Customers": 1000
    }
  },
  "Cookies": {
    "AccessTokenName": "accessToken",
    "RefreshTokenName": "refreshToken",
    "AccessTokenExpiryMinutes": 15,
    "RefreshTokenExpiryDays": 7,
    "Domain": "",
    "SecureInProduction": true,
    "SameSite": "Lax"
  }
}
